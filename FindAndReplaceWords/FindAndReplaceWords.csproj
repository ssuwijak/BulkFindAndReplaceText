<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <BaseOutputPath>r:\temp\vs2022\$(MSBuildProjectName)\bin\</BaseOutputPath>
    <ApplicationIcon>images\find-and-replace-icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="images\find-and-replace-icon.ico" />
  </ItemGroup>

</Project>